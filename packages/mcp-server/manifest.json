{
  "dxt_version": "0.1",
  "name": "limrun-v1-mcp",
  "version": "0.0.1",
  "description": "The official MCP Server for the Limrun API",
  "author": {
    "name": "Limrun",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stainless-sdks/limrun-v1-typescript.git"
  },
  "homepage": "https://github.com/stainless-sdks/limrun-v1-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "",
  "server": {
    "type": "node",
    "entry_point": "${__dirname}/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "LIM_TOKEN": "${user_config.LIM_TOKEN}"
      }
    }
  },
  "user_config": {
    "LIM_TOKEN": {
      "title": "api_key",
      "description": "",
      "required": false,
      "type": "string"
    }
  },
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
